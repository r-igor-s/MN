      "source": [
        "#Створіть одновимірний масив (вектор) з сумами кожного числа та його\n",
        "#попереднього, починаючи з 1, та виведіть його значення.\n",
        "import numpy as np\n",
        "\n",
        "n = 10\n",
        "\n",
        "numbers = np.arange(1, n + 1)\n",
        "previous = np.arange(0, n)\n",
        "result = numbers + previous\n",
        "\n",
        "print(result)"
      ]
